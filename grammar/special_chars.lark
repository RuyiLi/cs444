SQ_BRACKETS: "[]"

EQ: "=="
NOT_EQ: "!="
LOGICAL_OR: "||"
LOGICAL_AND: "&&"
EAGER_OR: "|"
EAGER_AND: "&"

LT_EQ: "<="
GT_EQ: ">="

LINE_TERMINATOR: "\r" | "\n" | "\r\n"
WHITESPACE: " " | "\f" | "\t"

INPUT_CHAR: /(?!(\r|\n))[\x00-\x7F]/

TRADITIONAL_COMMENT: "/" "*" COMMENT_TAIL

END_OF_LINE_COMMENT: "/" "/" INPUT_CHAR* LINE_TERMINATOR

COMMENT_TAIL: /[\x00-\x7F]*?(\*\/)/

%ignore TRADITIONAL_COMMENT
%ignore END_OF_LINE_COMMENT
%ignore LINE_TERMINATOR
%ignore WHITESPACE
